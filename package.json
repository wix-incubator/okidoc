  },
  "homepage": "https://github.com/wix-private/okidoc#readme",
  "dependencies": {
    "@babel/generator": "7.0.0-beta.38",
    "@babel/traverse": "7.0.0-beta.38",
    "@babel/types": "7.0.0-beta.38",
    "babylon": "7.0.0-beta.38",
    "cross-spawn": "^6.0.3",
    "documentation": "^5.3.5",
    "ejs": "^2.5.7",
    "glob": "^7.1.2",
    "lodash": "^4.17.4",
