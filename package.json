{
  "name": "okidoc",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "files": ["bin", "lib", "src"],
  "bin": {
    "okidoc": "./bin/okidoc.js",
    "okidoc-site": "./bin/okidoc-site.js"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib --copy-files",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --config .prettierrc --write '{src,bin}/**/*.js'",
    "precommit": "lint-staged",
    "postinstall": "cd site && npm install"
  },
  "lint-staged": {
    "*.{js,json,md}": ["prettier --config .prettierrc --write", "git add"]
  },
  "repository": {
    "type": "git",
